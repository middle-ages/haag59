{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "override",
  "bundledPackages": [],
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.json"
  },
  "apiReport": {
    "enabled": true,
    "reportFileName": "types-and-signatures.md",
    "reportFolder": "<projectFolder>/doc"
  },

  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/esm/types-and-signatures.json"
  },

  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/esm/<unscopedPackageName>.d.ts",
    "alphaTrimmedFilePath": "<projectFolder>/esm/<unscopedPackageName>-alpha.d.ts",
    "betaTrimmedFilePath": "<projectFolder>/esm/<unscopedPackageName>-beta.d.ts",
    "publicTrimmedFilePath": "<projectFolder>/esm/<unscopedPackageName>-public.d.ts"
  },

  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "<projectFolder>/esm/tsdoc-metadata.json"
  },

  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": false
      }
    },
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      }
    }
  }
}
